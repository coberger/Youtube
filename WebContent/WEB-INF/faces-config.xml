<faces-config xmlns="http://java.sun.com/xml/ns/javaee"	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"	version="2.0">		<navigation-rule>		<display-name>login/login.xhtml</display-name>		<from-view-id>/login/login.xhtml</from-view-id>		<navigation-case>			<from-outcome>loginSuccess</from-outcome>			<to-view-id>/app/main.xhtml</to-view-id>			<redirect />		</navigation-case>	</navigation-rule>		<navigation-rule>		<display-name>login/login.xhtml</display-name>		<from-view-id>/login/login.xhtml</from-view-id>		<navigation-case>			<from-outcome>loginFailed</from-outcome>			<to-view-id>/login/login.xhtml</to-view-id>		</navigation-case>	</navigation-rule>	<navigation-rule>		<from-view-id>login/login.xhtml</from-view-id>		<navigation-case>        	<from-action>#{compteBean.connexion}</from-action>         	<from-outcome>home</from-outcome>         	<to-view-id>index.xhtml</to-view-id>      	</navigation-case>   </navigation-rule>      <navigation-rule>		<from-view-id>login/signin.xhtml</from-view-id>		<navigation-case>        	<from-action>#{compteBean.inscrire}</from-action>         	<from-outcome>home</from-outcome>         	<to-view-id>index.xhtml</to-view-id>      	</navigation-case>   </navigation-rule>      <navigation-rule>		<from-view-id>index.xhtml</from-view-id>		<navigation-case>        	<from-action>#{compteBean.deconnexion}</from-action>         	<from-outcome>home</from-outcome>         	<to-view-id>index.xhtml</to-view-id>      	</navigation-case>   </navigation-rule>		<!-- Managed Beans -->	<managed-bean>	  <managed-bean-name>compteBean</managed-bean-name>	  <managed-bean-class>beans.CompteBean</managed-bean-class>	  <managed-bean-scope>session</managed-bean-scope>     </managed-bean>     	<managed-bean>	  <managed-bean-name>videoBean</managed-bean-name>	  <managed-bean-class>beans.VideoBean</managed-bean-class>	  <managed-bean-scope>session</managed-bean-scope>     </managed-bean>          	<managed-bean>	  <managed-bean-name>categorieBean</managed-bean-name>	  <managed-bean-class>beans.CategorieBean</managed-bean-class>	  <managed-bean-scope>session</managed-bean-scope>     </managed-bean>
</faces-config>